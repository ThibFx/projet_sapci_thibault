# Configuration de l'environnement
NODE_ENV=development
PORT=3000

# Base de donnees PostgreSQL (Render)
# Remplacer par l'URL de connexion Render
DATABASE_URL=postgresql://user:password@host:5432/pollutracker

# Secrets JWT (generer des cles aleatoires de 32+ caracteres)
JWT_ACCESS_SECRET=votre-secret-access-token-min-32-caracteres
JWT_REFRESH_SECRET=votre-secret-refresh-token-min-32-caracteres

# CORS
CORS_ORIGIN=http://localhost:4200

# Upload
UPLOAD_MAX_SIZE=5242880
